{% extends "admin/wrappers/_formDialog.twig" %}
{% block dialogHeader %}
    <h3 class="text-primary bold"><i class="pg-trash m-r-15"></i>Delete {{ title|capitalize }}</h3>
{% endblock %}
{% block dialogContent %}
    {{ form|raw }}
{% endblock %}
{% block dialogButtons %}
    <a class="btn btn-danger" onclick="return bootstrapModalSubmit();"><i class="pg-trash m-r-10"></i>Delete</a>
    <a class="btn" data-dismiss="modal"><i class="pg-close m-r-10"></i>Close</a>
{% endblock %}